{
    "name": "diadal/passport",
    "description": "Convert client_id to Uuid in Laravel Passport",
    "keywords": ["laravel", "framework", "Laravel Passport" ,"Uuid", "client Uuid"],
    "tag": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Diadal",
            "email": "info@diadal.com.ng"
        }
    ],
    "require": {
       "php": ">=7.1",
       "webpatser/laravel-uuid": "^3.0",
       "laravel/passport": "6.*|7.*"
    },
    "autoload": {
        "psr-4": {
            "Diadal\\Passport\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.7-dev"
        },
        "laravel": {
          "dont-discover": [
            "Diadal\\Passport\\PassportServiceProvider"
          ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
